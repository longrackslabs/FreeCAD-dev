{
  "name": "FreeCAD Dev",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/opt/FreeCAD",
  "postCreateCommand": "cd /opt/FreeCAD/build && ninja",
  "remoteUser": "root",
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
    "ms-vscode.cpptools",
    "ms-vscode.cmake-tools"
  ]
}

